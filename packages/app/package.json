{
	"name": "personal-time-punch",
	"version": "1.0.0",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"main": "./.vite/build/main.cjs",
	"description": "A replacement for Time Punch website made to be easy to use",
	"author": {
		"email": "matin.tat85@gmail.com",
		"name": "Matin Tat",
		"url": "https://github.com/WerdoxDev"
	},
	"scripts": {
		"dev": "bun ./scripts/dev.ts",
		"preview": "vite preview",
		"build": "bun run vite:build && bun run electron:build",
		"vite:dev": "vite",
		"vite:build": "cross-env NODE_ENV=production vite build",
		"electron:build": "bun ./scripts/build.ts && electron-builder --config ./electron-builder-config.ts",
		"electron:run": "bun ./scripts/build.ts && electron .",
		"build:fast": "bun run vite:build && bun ./scripts/build.ts && electron-builder --config ./electron-builder-config.ts -c.compression=store"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.354",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@types/bun": "latest",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"cross-env": "^7.0.3",
		"electron": "37.1.0",
		"electron-builder": "26.0.12",
		"tailwindcss": "^4.1.11",
		"vite": "npm:rolldown-vite@latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.4",
		"@tailwindcss/vite": "^4.1.11",
		"@tanstack/react-query": "^5.81.5",
		"@tanstack/react-table": "^8.21.3",
		"@vitejs/plugin-react-oxc": "^0.2.3",
		"clsx": "^2.1.1",
		"immer": "^10.1.1",
		"moment": "^2.30.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.6.3",
		"shared": "workspace:*",
		"unplugin-auto-import": "^19.3.0",
		"unplugin-icons": "^22.1.0",
		"zustand": "^5.0.6"
	}
}